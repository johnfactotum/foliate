          </object>
        </child>
        <child>
          <object class="GtkModelButton">
            <property name="visible">True</property>
            <property name="action-name">lib-book.export</property>
            <property name="text" translatable="yes">Export Annotationsâ€¦</property>
          </object>
        </child>
        <child>
          <object class="GtkSeparator">
            <property name="visible">True</property>
          </object>
        </child>
        <child>
          <object class="GtkModelButton">
            <property name="visible">True</property>
            <property name="action-name">lib-book.edit</property>
            <property name="text" translatable="yes">Edit</property>
          </object>
        </child>
        <child>
          <object class="GtkModelButton">
            <property name="visible">True</property>
            <property name="action-name">lib-book.remove</property>
            <property name="text" translatable="yes">Remove</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
